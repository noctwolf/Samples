<Window x:Class="依赖属性更改事件.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:依赖属性更改事件"
        mc:Ignorable="d"
        Title="MainWindow" Height="225" Width="400" WindowStartupLocation="CenterScreen">
    <StackPanel>
        <TextBox x:Name="textBox" Height="23" TextWrapping="Wrap" Text="TextBox"/>
        <TextBlock x:Name="textBlock" TextWrapping="Wrap" Text="TextBlock"/>
    </StackPanel>
</Window>
